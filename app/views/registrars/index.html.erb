<p id="notice"><%= notice %></p>

<h1>Registrars</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Fax</th>
      <th>Mail</th>
      <th>Url</th>
      <th>Adresse</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registrars.each do |registrar| %>
      <tr>
        <td><%= registrar.name %></td>
        <td><%= registrar.phone %></td>
        <td><%= registrar.fax %></td>
        <td><%= registrar.mail %></td>
        <td><%= registrar.url %></td>
        <td><%= registrar.adresse %></td>
        <td><%= registrar.city %></td>
        <td><%= link_to 'Show', registrar %></td>
        <td><%= link_to 'Edit', edit_registrar_path(registrar) %></td>
        <td><%= link_to 'Destroy', registrar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registrar', new_registrar_path %>
